<div id= families>

  <h1 class="family">My Family</h1>
  <div class="family_container">
    <% @families.each do |f| %>
     <div id= "fammember"
       <h3>  Name: <%= f.first_name %> <%= f.last_name  %></h3>
       <h3> Date of Birth: <%= f.dob %> </h3>
       <h3> Phone Number: <%=f.phone_number  %></h3>
       <h3> Email: <%= f.email  %></h3>
       <h3> Relationship: <%= f.relationship %></h3>
    </div>
    <% end %>
  </div>
</div>