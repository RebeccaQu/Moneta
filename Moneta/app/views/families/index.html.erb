<div id= families>

<h1 class="family">My Family</h1>

<% @families.each do |f| %>
 <div id= "fammember"
   <h3> <%= f.first_name %> <%= f.last_name  %></h3>
   <h3> <%= f.dob %> </h3>
   <h3> <%=f.phone_number  %></h3>
   <h3> <%= f.email  %></h3>
   <h3> <%= f.relationship %></h3>
</div>
<% end %>

</div>