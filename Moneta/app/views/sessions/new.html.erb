<h1>Hi there, please log in!</h1>

<div class="user-container">



  <%= form_tag sessions_path do %>
    <div class="field">
      <%= label_tag :email %><br/>
      <%= text_field_tag :email %>
    </div>

    <div class="field">
      <%= label_tag :password %><br/>
      <%= password_field_tag :password %>
    </div>
    <div id="sub" class="actions"><%= submit_tag "Log in" %></div>
  <% end %>
</div>